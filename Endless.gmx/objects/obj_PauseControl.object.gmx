    if(global.GameIsPaused == 0)
    { 
        global.GameIsPaused = 1
        global.IAmPaused = true
        
        with(obj_Reticle)
        {
            instance_destroy() //so it destroys the reticle everytimeyou pause
        }
            
        instance_deactivate_all(true);
        //create pause showing up stuff
        scr_WeaponBought() //run script showing weapons bought and not bought
        instance_create(view_xview[0] + 438, view_yview[0] + 992,obj_MainMenuPM)
        instance_create(view_xview[0] + 1632, view_yview[0] + 992,obj_QuitPS)
        instance_create(view_xview[0] + 640, view_yview[0] + 655,obj_AmmoPurchasePrice_Money)
